#include<stdio.h>
int main()
{
	int fact=1,n,i;
	printf("enter the number\n");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		fact=fact*i;
	}
	if(n>=1)
	printf("the given fact is %d of %d \n",n,fact);
	else if(n<0)
	printf("it is invalid input of negative numbers\n");
	else
	printf("character is invalid\n");
	return 0;
}
