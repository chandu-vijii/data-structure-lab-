#include<stdio.h>
 int main()
{
	int n1=0,n2 =1,n3,i,n;
	printf("enter number of elements \n");
	scanf("%d",&n);
	printf("\n%d%d",n1,n2);
	if(n>0)
 {
	for(i=2;i<n;i++)
	{
		n3=n1+n2;
		printf("%d",n3);
		n1=n2;
		n2=n3;
	}
 }
 else if(n<0)
 printf("negative is invalid");
 else
 printf("character is invalid");
	return 0;
}
