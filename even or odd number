#include<stdio.h>
int main()
{
	int n;
	printf("enter the number\n");
	scanf("%d",&n);
	if(n>0 || n<0)
{
	if(n%2==0)
	{
		printf("even number\n",n);
	}
	else 
	{
		printf("odd number \n",n);
	}
}
	//test case
	else 
	{
	printf("given is charcter is not allowed !\n ",n);
	printf("enter the valid input\n");
    }

	
	return 0;
}
