#include<stdio.h>
int main()
{
	int n,i,a=i;
	printf("enter the number \n");
	scanf("%d",&n);
	if(n>0)
   {
	for(i=1;i<n;i++)
	{
		if(n/i==i)
		printf("perfect number");
		else
		printf("not");
	}
  }
  else if(n<0)
  printf("negative is invalide");
  else
  printf("character is invalide");  
	return 0;
}
